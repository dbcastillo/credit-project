<%= form_for @creditcard do |form_helper| %>
  <%= form_helper.label :username %>
  <%= form_helper.text_field :username %>
  <%= form_helper.label :limit %>
  <%= form_helper.text_field :limit %>
  <%= form_helper.label :apr %>
  <%= form_helper.text_field :apr %>
  <%= form_helper.submit %>
<% end %>
